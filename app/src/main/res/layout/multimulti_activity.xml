<?xml version="1.0" encoding="utf-8"?>
<GridLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:columnCount="22"
    android:rowCount="22"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:id="@+id/multiMultiWybierzDateTetView"
        android:text="Wybierz datę początkową i końcową"
        android:layout_columnSpan="20"
        android:layout_row="0"
        android:layout_column="0"/>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:hint="Start"
        android:inputType="date"
        android:layout_columnSpan="8"
        android:id="@+id/multiMultiDataPierwszegoEditText"
        android:layout_row="2"
        android:layout_column="0" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:hint="Koniec"
        android:inputType="date"
        android:layout_columnSpan="8"
        android:id="@+id/multiMultiDataOstaniegoEditText"
        android:layout_row="2"
        android:layout_column="12" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:id="@+id/multiMultiIloscLiczbTextView"
        android:text="Ile liczb zaznaczyles"
        android:layout_columnSpan="20"
        android:layout_row="4"
        android:layout_column="0"/>
    <RadioGroup
        android:orientation="horizontal"
        android:id="@+id/multiMulti1Grupa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_columnSpan="20">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:checked="true"
            android:id="@+id/multiMulti1Radio" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2"
            android:id="@+id/multiMulti2Radio" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:id="@+id/multiMulti3Radio" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4"
            android:id="@+id/multiMulti4Radio" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5"
            android:id="@+id/multiMulti5Radio" />
    </RadioGroup>
    <RadioGroup
        android:orientation="horizontal"
        android:id="@+id/multiMulti2Grupa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_columnSpan="20">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6"
            android:id="@+id/multiMulti6Radio" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7"
            android:id="@+id/multiMulti7Radio" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8"
            android:id="@+id/multiMulti8Radio"/>

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9"
            android:id="@+id/multiMulti9Radio"
            />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10"
            android:id="@+id/multiMulti10Radio" />
    </RadioGroup>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:id="@+id/multiMultiPodajLiczbyTextView"
        android:text="Podaj swoje liczby"
        android:layout_columnSpan="20"
        android:layout_row="9"
        android:layout_column="0"/>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti1Liczba"
        android:layout_row="11"
        android:layout_column="1" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti2Liczba"
        android:visibility="invisible"
        android:layout_row="11"
        android:layout_column="4" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti3Liczba"
        android:visibility="invisible"
        android:layout_row="11"
        android:layout_column="7" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti4Liczba"
        android:visibility="invisible"
        android:layout_row="11"
        android:layout_column="10" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti5Liczba"
        android:visibility="invisible"
        android:layout_row="11"
        android:layout_column="13" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti6Liczba"
        android:visibility="invisible"
        android:layout_row="15"
        android:layout_column="1" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti7Liczba"
        android:visibility="invisible"
        android:layout_row="15"
        android:layout_column="4" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti8Liczba"
        android:visibility="invisible"
        android:layout_row="15"
        android:layout_column="7" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti9Liczba"
        android:visibility="invisible"
        android:layout_row="15"
        android:layout_column="10" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_rowSpan="3"
        android:id="@+id/multiMulti10Liczba"
        android:visibility="invisible"
        android:layout_row="15"
        android:layout_column="13" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel"
        android:onClick="finisActivity"
        android:id="@+id/multiMultiCancelButton"
        android:layout_columnSpan="8"
        android:layout_row="18"
        android:layout_column="1" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Save"
        android:layout_columnSpan="8"
        android:id="@+id/multiMultiSaveButton"
        android:onClick="saveLottoNumbers"
        android:layout_row="18"
        android:layout_column="11"/>


</GridLayout>