<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.tengradon.asystentgierlosowych.MainActivity"
    tools:showIn="@layout/activity_main">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/przycisk_nowy_kupon"
        android:id="@+id/addButton"
        android:layout_marginTop="32dp"
        android:onClick="pokazWyborGry"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="46dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/przycisk_najnowsze_wyniki"
        android:id="@+id/latestButton"
        android:onClick="pokazOstatnieWyniki"
        android:layout_alignTop="@+id/addButton"
        android:layout_toEndOf="@+id/addButton" />

    <FrameLayout
        android:id="@+id/fragment_place"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/addButton"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>
